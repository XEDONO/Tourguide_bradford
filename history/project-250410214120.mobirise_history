                    });\n\n                    paginationLinks.forEach(link => {\n                        link.parentElement.classList.remove('active');\n                        if (parseInt(link.getAttribute('data-page')) === pageNumber) {\n                            link.parentElement.classList.add('active');\n                        }\n                    });\n                }\n\n                paginationLinks.forEach(link => {\n                    // Remove previous listeners if any (important for Mobirise previews)\n                    // A bit complex, ideally Mobirise handles script re-runs, but this is a safety measure\n                    // const newLink = link.cloneNode(true);\n                    // link.parentNode.replaceChild(newLink, link);\n\n                    // Use 'newLink' if using the clone method above\n                    link.addEventListener('click', function(event) {\n                        event.preventDefault();\n                        event.stopPropagation(); // Prevent potential conflicts\n                        const pageNumber = parseInt(this.getAttribute('data-page'));\n                        if (!isNaN(pageNumber)) {\n                            showPage(pageNumber);\n\n                             // Optional: Scroll to the top of the blog posts container\n                            const containerTop = postsContainer.getBoundingClientRect().top + window.pageYOffset - 80; // Adjust offset\n                             window.scrollTo({ top: containerTop, behavior: 'smooth' });\n                        }\n                    });\n                });\n\n                // Show the first page initially\n                showPage(1);\n            }\n        });\n    </script>"
